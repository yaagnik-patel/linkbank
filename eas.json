{
  "cli": {
    "version": ">= 16.13.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "GRADLE_OPTS": "-Dorg.gradle.jvmargs=-Xmx4g -XX:MaxMetaspaceSize=512m",
        "KOTLIN_DAEMON_JVM_OPTIONS": "-Xmx2g"
      },
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "npm_config_legacy_peer_deps": "true"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}